/* back to top btn css */
#back-to-top {
    position: fixed;
    bottom: 100px;
    right: 10px;
    z-index: 9999;
    width: 32px;
    height: 32px;
    text-align: center;
    line-height: 30px;
    /**background: rgba(0, 0, 0, 0.55); **/
    color: #fff;
    cursor: pointer;
    border: 0;
    border-radius: 2px;
    text-decoration: none;
    transition: opacity 0.2s ease-out;
    opacity: 0;
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
}

#back-to-top:hover {
   /** background: #000000; **/
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
}

#back-to-top.show {
    opacity: 1;
}

