<?php
defined('BASEPATH') OR exit('No direct script access allowed');

class Mitra_school_seva_registration_web extends Base_Controller {
 
  public function __construct()
  {
    parent::__construct();
    $this->load->model('common_model');
  }

 public function mitra_school_seva_registration_list()
 {
   $data['mitra_school_seva_registration_data']=$this->common_model->fetchDataDesc('tbl_mitra_school_seva_registration','mitra_school_seva_registration_id');
   $data['keydata'] = $this->common_model->selectDetailsWhr('tbl_rest_keys','id',1);
   $this->load->view('mitra-school-seva-registration-list',$data);
 }

 public function reply_mitra_school_seva_registration($id=NULL)
 {
   $data['keydata'] = $this->common_model->selectDetailsWhr('tbl_rest_keys','id',1);
   $data['mitra_school_seva_registration_data'] = $this->common_model->addData('tbl_mitra_school_seva_registration',$data);
   
  if($id)
    {
      $data['mitra_school_seva_registration_data'] = $this->common_model->selectDetailsWhr(' tbl_mitra_school_seva_registration','mitra_school_seva_registration_id',$id);
    }
  $this->load->view('reply-mitra-school-seva-registration',$data);
 }
}
